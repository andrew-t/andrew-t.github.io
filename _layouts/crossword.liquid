<body>
	<header class="banner">
		<h1><a href="/">Andrew Taylor</a></h1>
		<nav class="tabs">
			<ul>
			{% for link in site.pages %}
			{% if link.tab %}
				<li><a href="{{ link.url }}" class="{% if link.url == page.url %}current{% endif %}">{{ link.title }}</a></li>
			{% endif %}
			{% endfor %}
			</ul>
		</nav>
	</header>
	<article>
		{% if page.addTitle %}
		<h1 class="page-title"><a href="{{ page.url }}">{{ page.title }}</a></h1>
		{% endif %}
		{{content}}
	</article>
	<footer class="banner">
